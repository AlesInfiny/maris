{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Dressca": "Information"
    }
  },
  "WebServerOptions": {
    "AllowedOrigins": [
      "https://[本番環境のフロントエンド配布サーバーのドメイン]"
    ],
    "CookieSettings": {
      "SameSite": "None",
      "HttpOnly": true,
      "Secure": true,
      "ExpiredDays": 7
    }
  },
  "AllowedHosts": "*"
}
