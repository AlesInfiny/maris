{
  "name": "consumer",
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "scripts": {
    "dev": "vite --mode dev",
    "mock": "vite --mode mock",
    "build:dev": "run-p type-check build-only:dev --print-label",
    "build:prod": "run-p type-check build-only:prod --print-label",
    "preview": "vite preview",
    "test:unit": "vitest",
    "prepare": "cypress install",
    "test:e2e": "start-server-and-test dev http://localhost:5173/ 'cypress open --e2e --browser chrome'",
    "test:e2e:ci": "start-server-and-test dev http://localhost:5173/ 'cypress run'",
    "build-only:dev": "vite build --mode dev",
    "build-only:prod": "vite build --mode prod",
    "type-check": "vue-tsc --build",
    "lint": "run-s eslint stylelint format --print-label",
    "lint:ci": "run-p eslint:ci format:ci stylelint:ci --print-label",
    "eslint": "eslint . --fix",
    "eslint:ci": "eslint .",
    "stylelint": "stylelint **/*.{vue,css} --fix",
    "stylelint:ci": "stylelint **/*.{vue,css}",
    "format": "prettier --write .",
    "format:ci": "prettier --check .",
    "generate-client": "openapi-generator-cli batch ./openapisettings.json --clean",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@heroicons/vue": "2.2.0",
    "@vueuse/core": "13.9.0",
    "axios": "1.12.2",
    "msw": "2.11.4",
    "pinia": "3.0.3",
    "vee-validate": "4.15.1",
    "vitest": "3.2.4",
    "vue": "3.5.22",
    "vue-i18n": "11.1.12",
    "vue-router": "4.5.1",
    "yup": "1.7.1"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  },
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "2.24.0",
    "@pinia/testing": "1.0.2",
    "@tailwindcss/vite": "4.1.14",
    "@tsconfig/node22": "22.0.2",
    "@types/jsdom": "27.0.0",
    "@types/node": "24.7.0",
    "@vitejs/plugin-vue": "6.0.1",
    "@vitejs/plugin-vue-jsx": "5.1.1",
    "@vitest/coverage-v8": "3.2.4",
    "@vitest/eslint-plugin": "1.3.16",
    "@vue/eslint-config-prettier": "10.2.0",
    "@vue/eslint-config-typescript": "14.6.0",
    "@vue/test-utils": "2.4.6",
    "@vue/tsconfig": "0.7.0",
    "cypress": "15.4.0",
    "eslint": "9.36.0",
    "eslint-plugin-cypress": "5.2.0",
    "eslint-plugin-jsdoc": "61.3.0",
    "eslint-plugin-vue": "10.5.0",
    "jiti": "2.6.1",
    "jsdom": "27.0.0",
    "npm-run-all2": "8.0.4",
    "prettier": "3.6.2",
    "prettier-plugin-tailwindcss": "0.6.14",
    "start-server-and-test": "2.1.2",
    "stylelint": "16.25.0",
    "stylelint-config-recommended-vue": "1.6.1",
    "stylelint-config-standard": "39.0.1",
    "tailwindcss": "4.1.14",
    "typescript": "5.8.3",
    "vite": "7.1.11",
    "vite-plugin-vue-devtools": "8.0.2",
    "vue-tsc": "3.1.1"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "4.52.4"
  }
}
