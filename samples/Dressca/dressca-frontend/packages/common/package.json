{
  "name": "@dressca-frontend/common",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "files": ["dist"],
  "exports": {
    ".": "./src/index.ts"
  },
  "scripts": {
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "test:unit": "vitest",
    "build-only:dev": "vite build --mode dev",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore && stylelint **/*.{vue,css} --fix",
    "lint:ci": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore && stylelint **/*.{vue,css}",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "@heroicons/vue": "^2.1.4",
    "vue": "^3.4.29"
  }
}
