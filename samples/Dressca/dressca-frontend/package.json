{
  "name": "dressca-frontend",
  "version": "1.0.0",
  "type": "module",
  "private": "true",
  "packageManager": "npm@10.9.3",
  "scripts": {
    "format:root": "prettier --write ./*.{js,ts,json,yaml}",
    "format:ci:root": "prettier --check ./*.{js,ts,json,yaml}",
    "lint:ci:consumer": "npm run lint:ci -w consumer",
    "type-check:consumer": "npm run type-check -w consumer",
    "build-only:dev:consumer": "npm run build-only:dev -w consumer",
    "build:prod:consumer": "npm run build:prod -w consumer",
    "test:unit:consumer": "npm run test:unit -w consumer",
    "dev:consumer": "npm run dev -w consumer",
    "mock:consumer": "npm run mock -w consumer",
    "lint:ci:admin": "npm run lint:ci -w admin",
    "type-check:admin": "npm run type-check -w admin",
    "build-only:dev:admin": "npm run build-only:dev -w admin",
    "build:prod:admin": "npm run build:prod -w admin",
    "test:unit:admin": "npm run test:unit -w admin",
    "dev:admin": "npm run dev -w admin",
    "mock:admin": "npm run mock -w admin",
    "test:coverage:consumer": "npm run coverage -w consumer",
    "test:coverage:admin": "npm run coverage -w admin"
  },
  "workspaces": [
    "consumer",
    "admin"
  ]
}
