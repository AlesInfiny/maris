{
  "name": "dressca-frontend",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --mode dev",
    "build:prod": "run-p typecheck build-only:prod --print-label",
    "build:dev": "run-p typecheck build-only:dev --print-label",
    "build-only:prod": "vite build --mode prod",
    "build-only:dev": "vite build --mode dev",
    "preview": "vite preview --port 5173",
    "test:unit": "vitest",
    "test:e2e": "start-server-and-test dev http://localhost:5173/ 'cypress open --e2e --browser chrome'",
    "test:e2e:ci": "start-server-and-test dev http://localhost:5173/ 'cypress run'",
    "typecheck": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore && stylelint **/*.{vue,css} --fix",
    "lint:ci": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore && stylelint **/*.{vue,css}",
    "mock": "vite --mode mock",
    "generate-client": "run-s openapi-client:clean openapi-client:generate --print-label",
    "openapi-client:clean": "node -e \"fs.promises.rm('./src/generated/api-client', {recursive: true, force: true})\"",
    "openapi-client:generate": "openapi-generator-cli generate -g typescript-axios -i ./../dressca-backend/src/Dressca.Web/dressca-api.json --additional-properties=withSeparateModelsAndApi=true,modelPackage=models,apiPackage=api,supportsES6=true -o ./src/generated/api-client"
  },
  "dependencies": {
    "@heroicons/vue": "^2.1.5",
    "@vue/eslint-config-airbnb-with-typescript": "^8.0.0",
    "axios": "^1.7.2",
    "pinia": "^2.2.0",
    "vee-validate": "^4.13.2",
    "vue": "^3.4.33",
    "vue-router": "^4.4.0",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "^2.13.4",
    "@rushstack/eslint-patch": "^1.10.4",
    "@tsconfig/node20": "^20.1.4",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^20.14.11",
    "@vitejs/plugin-vue": "^5.1.1",
    "@vitejs/plugin-vue-jsx": "^4.0.0",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.5.1",
    "autoprefixer": "^10.4.19",
    "cypress": "^13.13.1",
    "eslint": "^8.57.0",
    "eslint-plugin-cypress": "^3.4.0",
    "eslint-plugin-vue": "^9.27.0",
    "jsdom": "^24.1.1",
    "npm-run-all2": "^6.2.2",
    "postcss": "^8.4.40",
    "postcss-nesting": "^12.1.5",
    "prettier": "^3.3.3",
    "start-server-and-test": "^2.0.5",
    "stylelint": "^16.8.1",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-config-standard": "^36.0.1",
    "stylelint-prettier": "^5.0.2",
    "tailwindcss": "^3.4.7",
    "typescript": "~5.4.5",
    "vite": "^5.3.5",
    "vitest": "^2.0.3",
    "vue-tsc": "^2.0.29"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "^4.20.0"
  }
}
