{
  "AzureAdB2C": {
    "Instance": "https://{your-tenant-name}.b2clogin.com",
    "ClientId": "application-client-id",
    "Domain": "{your-tenant-name}.onmicrosoft.com",
    "SignedOutCallbackPath": "{signout-callback-path}",
    "SignUpSignInPolicyId": "{policy_id}",
    "ResetPasswordPolicyId": "{policy_id}",
    "EditProfilePolicyId": "{policy_id}"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Dressca": "Information"
    }
  },
  "AllowedHosts": "*"
}
