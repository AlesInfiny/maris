{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "DresscaCMS": "Information"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "AnnouncementDbContext": "Data Source=(localdb)\\mssqllocaldb;Database=Dressca.Cms.Announcement;Integrated Security=True",
    "AuthenticationDbContext": "Data Source=(localdb)\\mssqllocaldb;Database=Dressca.Cms.Authentication;Integrated Security=True"
  },
  "MaxRequestSize": {
    "MaxRequestBodySizeBytes": 3145728,
    "MultipartBodyLengthLimit":  10485760
  }
}
