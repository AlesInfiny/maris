{
  "name": "app",
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": "^24.13.0",
    "npm": "^11.6.2"
  },
  "scripts": {
    "dev": "vite --mode dev",
    "mock": "vite --mode mock",
    "build:dev": "run-p type-check build-only:dev --print-label",
    "build:prod": "run-p type-check build-only:prod --print-label",
    "preview": "vite preview",
    "test:unit": "vitest",
    "type-check": "vue-tsc --build",
    "lint": "run-s eslint format --print-label",
    "lint:ci": "run-p eslint:ci format:ci --print-label",
    "build-only:dev": "vite build --mode dev",
    "build-only:prod": "vite build --mode prod",
    "eslint": "eslint . --fix --cache",
    "eslint:ci": "eslint . --cache",
    "format": "prettier --write .",
    "format:ci": "prettier --check .",
    "generate-client": "openapi-generator-cli batch ./openapisettings.json --clean",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@azure/msal-browser": "4.27.0",
    "axios": "1.13.5",
    "msw": "2.12.4",
    "pinia": "3.0.4",
    "vitest": "3.2.4",
    "vue": "3.5.25"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  },
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "2.25.2",
    "@pinia/testing": "1.0.3",
    "@tsconfig/node24": "^24.0.3",
    "@types/jsdom": "27.0.0",
    "@types/node": "25.0.2",
    "@vitejs/plugin-vue": "6.0.3",
    "@vitejs/plugin-vue-jsx": "5.1.2",
    "@vitest/coverage-v8": "3.2.4",
    "@vitest/eslint-plugin": "1.5.2",
    "@vue/eslint-config-prettier": "10.2.0",
    "@vue/eslint-config-typescript": "14.6.0",
    "@vue/test-utils": "2.4.6",
    "@vue/tsconfig": "0.7.0",
    "eslint": "9.39.2",
    "eslint-plugin-jsdoc": "61.5.0",
    "eslint-plugin-vue": "10.6.2",
    "jiti": "2.6.1",
    "jsdom": "27.3.0",
    "npm-run-all2": "8.0.4",
    "prettier": "3.7.4",
    "typescript": "5.8.3",
    "vite": "7.3.0",
    "vite-plugin-vue-devtools": "8.0.5",
    "vue-tsc": "3.1.8"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "4.53.4"
  }
}
